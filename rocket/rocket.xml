<mujoco>
    <asset>
      <texture name="grid" type="2d" builtin="checker" rgb1=".1 .2 .3"
       rgb2=".2 .3 .4" width="300" height="300" mark="none"/>
      <material name="grid" texture="grid" texrepeat="1 1"
       texuniform="true" reflectance=".2"/>
    </asset>
  
    <worldbody>
  
      <geom name="floor" type="plane" pos="0 0 -.5" size="2 2 .1" material="grid"/>
  
  
      <body name="rocket" pos="5 0 -.2" euler="0 0 0">
        <light name="light" mode = "track" pos="0 0 1"/>
        <camera name="fixed" mode = "track" pos="0 -1.3 .5" xyaxes="1 0 0 0 1 2"/>
        <joint name="free" type="free"/>
        <geom name="body" type="cylinder" size=".075 .2 .1" rgba="1 0 0 1"/>
        <site name="thrust_point" pos="0 0 -0.2" size="0.01"/>
      </body>
  
    </worldbody>
  
  
    <actuator>
      <motor name="thrust_x" site="thrust_point" gear="1 0 0 0 0 0" ctrlrange="0 70"/>
      <motor name="thrust_y" site="thrust_point" gear="0 1 0 0 0 0" ctrlrange="0 70"/>
      <motor name="thrust_z" site="thrust_point" gear="0 0 1 0 0 0" ctrlrange="0 70"/>
    </actuator>
  
  </mujoco>